{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "lint:ts": "tsc --noEmit",
    "format:yaml": "prettier \"**/*.{yaml,yml}\"",
    "format:yaml:ci": "prettier \"**/*.{yaml,yml}\" !pnpm-lock.yaml",
    "format:yaml:fix": "prettier --write \"**/*.{yaml,yml}\"",
    "start": "remix-serve ./build/server/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc",
    "preinstall": "npx typesync || :",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "isbot": "^5.1.17",
    "lucide-react": "^0.416.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.3",
    "@commitlint/cli": "^19.5.0",
    "@remix-run/dev": "^2.12.1",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "~21.1.7",
    "@types/lint-staged": "~13.3.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vanilla-extract/css": "^1.16.0",
    "@vanilla-extract/vite-plugin": "^4.0.16",
    "@vitejs/plugin-react": "^4.3.2",
    "@vitest/coverage-v8": "^2.1.2",
    "@vitest/ui": "^2.1.2",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "cz-customizable": "^7.2.1",
    "husky": "^9.1.6",
    "jsdom": "^24.1.3",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "typesync": "^0.13.0",
    "vite": "^5.4.8",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.cts"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,cjs,mjs,ts,tsx,cts,mts,json,md}": "biome check --write",
    "**/*.{yaml,yml}": "prettier --write"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "20.17.0",
    "pnpm": "9.11.0"
  }
}
